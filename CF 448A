#include <bits\stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string a,b;
    cin>>a>>b;
    int sum=0;
    for(int i=0 ; i < n ; i++){
        if(abs(int(a[i])-int(b[i]))<=5) sum=sum+abs(int(a[i])-int(b[i]));
        else sum=sum+(10-abs(int(a[i])-int(b[i])));
    }
    cout<<sum<<endl;

    //cout<<(int(a[0])-int(b[0]));
    return 0;
}
